{
  "name": "nextbone-routing",
  "version": "0.14.0",
  "description": "A state based router for Nextbone applications",
  "homepage": "https://github.com/blikblum/nextbone-routing",
  "repository": "blikblum/nextbone-routing",
  "author": "Luiz Américo Pereira Câmara",
  "contributors": [
    "Luiz Américo Pereira Câmara"
  ],
  "license": "MIT",
  "keywords": [],
  "main": "dist/nextbone-routing.js",
  "module": "dist/nextbone-routing.js",
  "sideEffects": false,
  "dependencies": {
    "slick-router": "^3.0.0"
  },
  "peerDependencies": {
    "lodash-es": "^4.17.21",
    "nextbone": "^0.28.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@open-wc/semantic-dom-diff": "^0.20.1",
    "@open-wc/testing-helpers": "^1.4.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@web/dev-server-esbuild": "^1.0.2",
    "@web/dev-server-rollup": "^0.6.3",
    "@web/test-runner": "^0.18.2",
    "@web/test-runner-puppeteer": "^0.16.0",
    "chai": "^5.1.1",
    "del": "^5.1.0",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.4.0",
    "jquery": "^3.4.1",
    "lit-element": "^2.2.1",
    "lodash-es": "^4.17.21",
    "mocha": "^6.2.2",
    "nextbone": "^0.28.3",
    "prettier": "^3.3.2",
    "rollup": "1.6.0",
    "sinon": "^8.0.2",
    "sinon-chai-es": "^3.7.0"
  },
  "scripts": {
    "lint": "eslint",
    "format": "prettier --write ./src ./test",
    "test": "web-test-runner \"test/**/*.js\" --node-resolve --puppeteer",
    "test:coverage": "web-test-runner \"test/**/*.js\" --node-resolve --coverage",
    "test:ts-decorator": "web-test-runner \"test/render.js\" --config=web-test-runner.esbuild.config.mjs --node-resolve --puppeteer",
    "build": "npm run lint && node tools/build",
    "prepublishOnly": "npm run build"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "browser": true
    }
  },
  "packageManager": "yarn@4.3.0"
}